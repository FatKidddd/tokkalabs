{
  "name": "tokkalabs",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "tsx src/server.ts",
    "build": "tsup src",
    "start:dev": "tsx watch src/server.ts",
    "husky:prepare": "husky install",
    "test": "vitest",
    "test:lint": "vitest run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn eslint --fix",
      "yarn prettier --write \"src/**/*.{ts,tsx}\"",
      "yarn test:lint --passWithNoTests"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bn.js": "^5",
    "@types/express": "^5.0.0",
    "@types/lodash": "^4",
    "@types/node": "^20.10.5",
    "@types/pg": "^8",
    "@types/swagger-jsdoc": "^6",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/ws": "^8",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "express": "^4.18.2",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "swagger-jsdoc": "^6.2.8",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "bn.js": "^5.2.1",
    "dotenv": "^16.4.5",
    "lodash": "^4.17.21",
    "pg": "^8.13.0",
    "swagger-ui-express": "^5.0.1",
    "tiny-invariant": "^1.3.3",
    "web3": "^4.13.0",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  }
}
